    int recursiveFactorial(int n) {
        if (n <= 1) {
            return 1;
        } else {
            return n * recursiveFactorial(n - 1);
        }
    }
    
    int recursiveFibonacci(int n) {
        if (n == 0) return 0;
        if (n == 1) return 1;
        return recursiveFibonacci(n - 1) + recursiveFibonacci(n - 2);
    }
    
