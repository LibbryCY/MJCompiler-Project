// Test FULL semantic coverage

program testFull

const int jedan = 1;
const char slovo = 'A';
const bool tacno = true;

int i, j;
bool flag;
char ch;

int niz[];

enum Days { MONDAY = 1, WEDNESDAY = 3, FRIDAY }

{

    int add(int a, int b)
    {
 
        return a + b;
    }

    int inc(int x)
    {
    	x = add;
        return x + 1;
    }

    bool verify()
    {
        print('v');
        return false;
    }

    void noPars()
    {
        print(1);
    }

    void main()
    {
        // assignment + constants
        i = 0;
        j = jedan;
        flag = tacno;
        ch = slovo;

        // method calls
        i = add(2,3);
        noPars();

        // ternary
        print(flag ? 1 : 0);

        // IF / ELSE
        if(i > 0)
            print(i);
        else
            print(0);

        // nested if (dangling else)
        if(flag)
            if(i == 5) print(5);
            else print(-5);

        // arrays
        niz = new int[5];
        niz[0] = 1;
        niz[1] = 2;
        niz[2] = add(niz[0], niz[1]);

        print(niz[2]);

        // length
        for(i = 0; i < niz.length; i++)
        {
            print(i);
        }

        // for without init and step
        i = 0;
        for(; i < 10;)
        {
            if(i == 3)
            {
                i++;
                continue;
            }

            if(i == 6)
                break;

            i = inc(i);
        }

        // logical expressions
        if(i == 6 || verify())
            print(6);
        else
            print(-6);

        // AND condition
        if(i == 6 && flag)
            print(66);

        // read / print
        read(niz[0]);
        print(niz[0]);

        // enum usage
        i = Days.MONDAY;
        if(i == 1)
            print(11);

        // switch test
        for(i = 0; i < 6; i++)
        {
            switch(i)
            {
                case 0:
                    print(0);
                    break;

                case 1:
                    print(1);

                case 2:
                    print(2);
                    break;

                case 3:
                    print(3);
                    continue;

                case 4:
                    print(4);

                case 5:
                    print(5);
            }

            print(eol);
        }

        // inc / dec
        i++;
        i--;

        print(eol);
    }
}
